---
import Layout from '../layouts/Layout.astro'
---

<Layout title="AWFixer Wiki">
  <div class="max-w-5xl mx-auto z-10">
    <div class="mb-12">
      <h1 class="text-4xl font-semibold mb-4 text-gray-900">
        AWFixer's Wiki
      </h1>
      <p class="text-xl text-gray-600">
        Find technical guides, documentation, tutorials, code examples,
        troubleshooting tips, and development resources.
      </p>
    </div>
  
    <div class="grid grid-cols-1 gap-4 md:grid-cols-2 md:gap-4">
      <a href="/overview/getting-started" class="block">
        <div class="group relative border border-gray-100 dark:border-gray-200 rounded-lg transition-all duration-200 cursor-pointer overflow-hidden h-40 md:h-56">
          <h3 class="text-lg font-semibold text-gray-900 dark:text-white">
            Quick Start
          </h3>
          <p class="mt-2 text-gray-600 dark:text-gray-400">
            Get up and running in minutes with our comprehensive quick start guide.
          </p>
        </div>
      </a>
    </div>
  </Layout>